<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">

	<select id="checkDuplication" resultType="Integer">
		SELECT
			COUNT(*)
		FROM
			USERS
		WHERE 1 = 1
		AND EMAIL = #{email}
	</select>
	
</mapper>